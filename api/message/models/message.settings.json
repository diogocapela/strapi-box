{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "messages",
  "info": {
    "name": "message"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "message": {
      "type": "text",
      "required": true
    },
    "name": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "phone": {
      "type": "biginteger"
    }
  }
}
