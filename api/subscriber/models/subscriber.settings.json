{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "subscribers",
  "info": {
    "name": "subscriber"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "email": {
      "type": "email",
      "required": true,
      "unique": true,
      "maxLength": 255
    },
    "name": {
      "type": "string",
      "maxLength": 255
    }
  }
}
